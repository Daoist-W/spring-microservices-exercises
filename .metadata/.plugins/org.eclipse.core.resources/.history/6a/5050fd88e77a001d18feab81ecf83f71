appender.Console.name=Console
appender.Console.type=Console
appender.Console.target=SYSTEM_OUT
appender.Console.layout.type=PatternLayout
appender.Console.layout.pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
appender.File.name=File
appender.File.type=File
appender.File.fileName=application.log
appender.File.layout.type=PatternLayout
appender.File.layout.pattern="%d{yyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"

logger.Console.name=Console
logger.Console.level=info
logger.Console.appenderRef.ref=Console
logger.File.name=File
logger.File.level=info
logger.File.appenderRef.ref=File

rootLogger.level=error
rootLogger.appenderRef.ref=Console
