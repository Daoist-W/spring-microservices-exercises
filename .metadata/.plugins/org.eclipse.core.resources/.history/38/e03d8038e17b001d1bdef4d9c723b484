  application:
    name: userMS
  cloud:
    consul:
      config:
        enabled: true
        prefix: configuration
        defaultContext: apps
        profileSeparator: '::'

  datasource:
    username: demo
    password: demo
    url: jdbc:mysql://localhost:3306/infygo_user
  jpa:
    show-sql: true
    hibernate:
      ddl-auto: update   
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQL5Dialect
